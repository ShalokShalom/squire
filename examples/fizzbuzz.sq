n = 1

whilst n < 101 {
	if !(n % 3) { print('Fizz') }
	if !(n % 5) { print('Buzz') }
	if (n%3)*(n%5) { print(n) }
	print("\n")
	n = n + 1
	if 3 < n {
		exit(0)
	}
}
