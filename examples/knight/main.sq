#!/usr/bin/env squire

@compile "assert.sq"
$assert(yay, "oops");


@compile "./variable.sq"
__END__
@compile "./parse.sq"
@compile "./functions.sq"

fork ARGV.length() == III && ARGV[II] {
	path '-e':
		expr = ARGV[III];

	path '-f':
		expr = hex("cat \(ARGV[III])");

	alas:
		proclaim("usage: \(ARGV[I]) (-e 'expr' | -f filename)\n")
		dismount(1);
}

parse(expr).run()
