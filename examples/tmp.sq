journey divide(n, d) {
	if !d {
		hark "cannot divide by zero";
	}

	reward n / d;
}

journey halve(num) {
	reward divide(num, 2);
}

attempt {
	dump(divide(2,3))
} retreat err {

	#dump(err)
}

__END__
