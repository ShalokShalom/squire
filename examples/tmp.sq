journey divide(n, d) {
	if !d {
		throw "cannot divide by zero";
	}

	reward n / d;
}

journey halve(num) {
	reward divide(num, 0);
}

try {
	dump(halve(2))
} catch err {
	throw;
	#dump(err)
}

